<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xqf.mapper.MovieMapper">

    <resultMap id="BaseResultMap" type="com.xqf.domain.entity.Movie">
            <id property="movieId" column="movie_id" jdbcType="OTHER"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="poster" column="poster" jdbcType="VARCHAR"/>
            <result property="director" column="director" jdbcType="VARCHAR"/>
            <result property="actor" column="actor" jdbcType="VARCHAR"/>
            <result property="movieLong" column="movie_long" jdbcType="VARCHAR"/>
            <result property="language" column="language" jdbcType="VARCHAR"/>
            <result property="intro" column="intro" jdbcType="VARCHAR"/>
            <result property="type" column="type" jdbcType="VARCHAR"/>
            <result property="publicDate" column="public_date" jdbcType="VARCHAR"/>
            <result property="wishNum" column="wish_num" jdbcType="OTHER"/>
            <result property="score" column="score" jdbcType="DECIMAL"/>
    </resultMap>

    <sql id="Base_Column_List">
        movie_id,name,poster,
        director,actor,movie_long,
        language,intro,type,
        public_date,wish_num,score
    </sql>

    <select id="getMovieList" resultType="com.xqf.domain.Vo.MovieRelease">

    </select>
</mapper>
